# DVC Remote Storage Configuration
# Uncomment and configure for cloud storage

# =============================================================================
# Local Storage (default - for development)
# =============================================================================
# Data is stored in .dvc/cache locally

# =============================================================================
# AWS S3 (Production)
# =============================================================================
# [core]
#     remote = s3storage
# ['remote "s3storage"']
#     url = s3://your-bucket/dvc-storage
#     region = us-east-1

# =============================================================================
# Google Cloud Storage
# =============================================================================
# [core]
#     remote = gcs
# ['remote "gcs"']
#     url = gs://your-bucket/dvc-storage

# =============================================================================
# Azure Blob Storage
# =============================================================================
# [core]
#     remote = azure
# ['remote "azure"']
#     url = azure://your-container/dvc-storage

# =============================================================================
# Local Network Storage (NAS/Shared Drive)
# =============================================================================
# [core]
#     remote = local
# ['remote "local"']
#     url = /mnt/shared/dvc-storage

# =============================================================================
# Current Configuration
# =============================================================================
[core]
    autostage = true
